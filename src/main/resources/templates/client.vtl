

#if($client)
<h2>$client</h2>
#end

<h2>Add Client</h2>
<form action='/clients' method="post">
  <div class="form-group">
    <label for="client">Client: </label>
    <input id="client" name="client" class="form-control" type="text">
  </div>

  <button type='submit' class='btn'>Add Client!</button>
</form>
